{
    "files.exclude": {
        "node_modules": true,
        "LICENSE": true,
        "dist": true
    },
    "files.associations": {
        "*.js": "javascriptreact"
    },
    "javascript.implicitProjectConfig.checkJs": true,
    "editor.formatOnSave": true,
    "editor.tabSize": 4,
    "editor.insertSpaces": true,
    "files.insertFinalNewline": true,
    "files.trimTrailingWhitespace": true,
    "files.trimFinalNewlines": true,
    "cSpell.words": [
        "Calibri",
        "HRMS",
        "POCs",
        "Sidenav",
        "browserslist",
        "dropdown",
        "homescreen",
        "inputtext",
        "linebreak",
        "minified",
        "noopener",
        "noreferrer",
        "primeflex",
        "primeicons",
        "primereact",
        "reactstrap",
        "sangam",
        "treeview",
        "webfonts",
        "zindex"
    ]
}
